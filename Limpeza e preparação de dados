        "def is_default(value):\n",
        "    return 1 if value in ['2', '3', '4', '5'] else 0\n",
        "\n",
        "\n",
        "mes_cols = [col for col in pg.columns if col.startswith(\"mes_\")]\n",
        "\n",
        "\n",
        "pg_default = pg.copy()\n",
        "pg_default[mes_cols] = pg[mes_cols].applymap(is_default)\n",
        "\n",
        "pg_default.head()\n"
      ]
